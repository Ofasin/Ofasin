<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

<application>
    <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
</application>

<managed-bean>
    <managed-bean-name>dominioUserLogin</managed-bean-name>
    <managed-bean-class>com.ofasin.os.ebanking.model.DominioUsers</managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
</managed-bean>

<managed-bean>
    <managed-bean-name>beanMenu</managed-bean-name>
    <managed-bean-class>com.ofasin.os.ebanking.bean.BeanMenu</managed-bean-class>

    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
        <property-name>user</property-name>
        <value>#{dominioUserLogin}</value>
    </managed-property>
</managed-bean>

<managed-bean>
    <managed-bean-name>modelImagenes</managed-bean-name>
    <managed-bean-class>com.ofasin.os.ebanking.model.DominioImagenes</managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
</managed-bean>

<managed-bean>
    <managed-bean-name>beanWelcome</managed-bean-name>
    <managed-bean-class>com.ofasin.os.ebanking.bean.BeanWelcome</managed-bean-class>

    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
        <property-name>imagen</property-name>
        <value>#{modelImagenes}</value>
    </managed-property>
    <managed-property>
        <property-name>service</property-name>
        <value>#{serviceImagenes}</value>
    </managed-property></managed-bean>
    
    <managed-bean>
    <managed-bean-name>modeltipEducacion</managed-bean-name>
    <managed-bean-class>com.ofasin.os.ebanking.model.DominioTipEducacion</managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
</managed-bean>

<managed-bean>
   <managed-bean-name>beanTipEducacion</managed-bean-name>
    <managed-bean-class>com.ofasin.os.ebanking.bean.BeanTipEducacion</managed-bean-class>

    <managed-bean-scope>session</managed-bean-scope> 
    
    <managed-property>
        <property-name>tipEducacion</property-name>
        <value>#{modeltipEducacion}</value>
    </managed-property>
    <managed-property>
        <property-name>service</property-name>
        <value>#{serviceTipEducacion}</value>
    </managed-property>
</managed-bean>
    
    
    
<managed-bean>
    <managed-bean-name>modeltipProfesion</managed-bean-name>
    <managed-bean-class>com.ofasin.os.ebanking.model.DominioTipProfesion</managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
</managed-bean>

<managed-bean>
   <managed-bean-name>beanTipoProfesion</managed-bean-name>
    <managed-bean-class>com.ofasin.os.ebanking.bean.BeanTipProfesion</managed-bean-class>

    <managed-bean-scope>session</managed-bean-scope> 
    
    <managed-property>
        <property-name>tipProfesion</property-name>
        <value>#{modeltipProfesion}</value>
    </managed-property>
    <managed-property>
        <property-name>service</property-name>
        <value>#{serviceTipProfesion}</value>
    </managed-property>
</managed-bean>

<managed-bean>
    <managed-bean-name>modelResguardo</managed-bean-name>
    <managed-bean-class>com.ofasin.os.ebanking.model.DominioResguardo</managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
</managed-bean>

<managed-bean>
   <managed-bean-name>beanResguardo</managed-bean-name>
    <managed-bean-class>com.ofasin.os.ebanking.bean.BeanResguardo</managed-bean-class>

    <managed-bean-scope>session</managed-bean-scope> 
    
    <managed-property>
        <property-name>resguardo</property-name>
        <value>#{modelResguardo}</value>
    </managed-property>
    <managed-property>
        <property-name>service</property-name>
        <value>#{serviceResguardo}</value>
    </managed-property>
</managed-bean>


<managed-bean>
    <managed-bean-name>modelAsociacion</managed-bean-name>
    <managed-bean-class>com.ofasin.os.ebanking.model.DominioAsociacion</managed-bean-class>
    <managed-bean-scope>none</managed-bean-scope>
</managed-bean>

<managed-bean>
   <managed-bean-name>beanAsociacion</managed-bean-name>
    <managed-bean-class>com.ofasin.os.ebanking.bean.BeanAsociacion</managed-bean-class>

    <managed-bean-scope>session</managed-bean-scope> 
    
    <managed-property>
        <property-name>asociacion</property-name>
        <value>#{modelAsociacion}</value>
    </managed-property>
    <managed-property>
        <property-name>service</property-name>
        <value>#{serviceAsociacion}</value>
    </managed-property>
</managed-bean>
</faces-config>
